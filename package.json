{
  "private": true,
  "scripts": {
    "start": "next start -p 3001",
    "db:migrate": "sequelize db:migrate --env database --migrations-path ./src/apis/database/migrations",
    "db:migrate:undo": "sequelize db:migrate:undo:all --env database --migrations-path ./src/apis/database/migrations",
    "db:seed": "sequelize db:seed:all --env database --seeders-path ./src/apis/database/seeders",
    "db:seed:undo": "sequelize db:seed:undo:all --env database --seeders-path ./src/apis/database/seeders",
    "db:reset": "npm run db:seed:undo && npm run db:migrate:undo && npm run db:migrate && npm run db:seed",
    "db:create": "sequelize db:create --env database",
    "db:drop": "sequelize db:drop --env database"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.29.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "formidable": "^2.0.1",
    "iron-session": "^6.1.3",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mime": "^3.0.0",
    "mysql2": "^2.3.3",
    "next": "latest",
    "node-fetch": "^3.2.10",
    "nodemailer": "^6.7.7",
    "nprogress": "^0.2.0",
    "pg": "^8.7.3",
    "react": "18.1.0",
    "react-doc-viewer": "^0.1.5",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.31.3",
    "react-i18next": "^11.17.0",
    "react-in-viewport": "^1.0.0-alpha.28",
    "react-pdf": "^5.7.2",
    "react-quill": "^1.3.5",
    "react-redux": "^8.0.2",
